{
  "manifest_version": 2,
  "name": "Open Classic",
  "description": "Open Links in Classic",
  "version": "0.8.2",
  "icons": {
    "16": "img/img_16.png",
    "48": "img/icon2.png",
    "128": "img/icon.png"
  },
  "browser_action": {
    "default_icon": "/img/img_16.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "/src/background.js"
    ]
  },
  "permissions": ["storage", "tabs", "webNavigation"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"

}